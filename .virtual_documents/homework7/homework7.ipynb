import numpy as np
import matplotlib.pyplot as plt


x = np.linspace(0, 2*np.pi, 1000)
def make_sine_wave(z, A, w):
    y = A*np.sin(z*w)
    return y

y = make_sine_wave(x, 1, 1)
plt.plot(x, y)
plt.show()


