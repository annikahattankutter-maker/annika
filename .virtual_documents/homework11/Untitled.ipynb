import numpy as np

import matplotlib.pyplot as plt


image = np.loadtxt('astro_image (1).csv')

plt.figure(figsize=(8,6))
plt.imshow(image, cmap='inferno', origin='lower', vmin = 0, vmax = 300)
plt.colorbar()     # add a colorbar
plt.title("Astro Image")
plt.xlabel("X Pixel")
plt.ylabel("Y Pixel")
plt.show()


zoom1 = image[200:350, 250:400]   # first zoom box
zoom2 = image[400:550, 300:450]   # second zoom box

fig, axes = plt.subplots(1, 2, figsize=(10,5))

# First zoom panel
axes[0].imshow(zoom1, cmap='gray', origin='lower')
axes[0].set_title("Zoom-in 1")
axes[0].axis('off')

# Second zoom panel
axes[1].imshow(zoom2, cmap='gray', origin='lower')
axes[1].set_title("Zoom-in 2")
axes[1].axis('off')

plt.tight_layout()
plt.show()



